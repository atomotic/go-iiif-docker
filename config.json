{
    "level": {
    	"compliance": "2"
    },
    "graphics": {
	"source": { "name": "VIPS" }
    },
    "features": {
	"enable": {
	    "size": [ "max" ],
	    "format": [ "webp", "tif", "gif" ]
	},
	"disable": {
	    "size": [ "sizeByDistortedWh" ] ,
	    "rotation": [ "rotationArbitrary" ],
	    "quality": [ "bitonal" ]
	},
	"append": {
	    "quality": {
		"dither": { "syntax": "dither", "required": false, "supported": true, "match": "^dither$" }
	    }
	}
    },
    "images": {
	"source": { "name": "Disk", "path": "/images" },
	"cache": { "name": "Memory", "ttl": 300, "limit": 100 }
    },
    "derivatives": {
	"cache": { "name": "Disk", "path": "/cache" }
    }
}
